<h1>Listing articles</h1>
 <%= link_to 'New article', new_article_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 <h1>Hello, Rails!</h1>
<%= link_to 'Blog', controller: 'articles' %>

  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
    </tr>
  <% end %>
</table>